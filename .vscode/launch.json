{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell Launch Script",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}/scripts/serve.ps1",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "minecraft-js",
      "request": "attach",
      "name": "Attach to Minecraft",
      "mode": "connect",
      "localRoot": "${workspaceFolder}/",
      "host": "localhost",
      "port": 19144
    },
    {
      "type": "kubejs",
      "request": "attach",
      "name": "KubeJS Attach",
      "hostName": "localhost",
      "port": 8000,
      "timeout": 30000
    }
  ]
}
